  "description": "SSLATT backend",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node build/index",
    "dev": "nodemon src/index.ts",
    "test": "mocha src/*/*test.ts --exit",
    "clean": "rm -rf build",
